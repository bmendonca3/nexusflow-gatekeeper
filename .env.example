# NexusFlow Gatekeeper Environment Configuration
# ------------------------------------------------
# Copy this file to .env and fill in your values
# NEVER commit real secrets to version control

# Server-Side Secret (REQUIRED - Server will fail without it)
NEXUS_SECRET=your-super-secret-key-here

# Client-Side Token (REQUIRED - Exposed to browser via Vite)
VITE_NEXUS_TOKEN=your-client-token-here

# Server Port (optional, default: 3001)
PORT=3001

# Node Environment
NODE_ENV=development
